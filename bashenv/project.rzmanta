#!/bin/bash
PS3='Please enter session choice: '
options=("umpire" "ompd")
select opt in "${options[@]}"
do
  case $opt in
      "ompd")
          export MJM_SESSION=$opt
          break
          ;;
      "umpire")
          export MJM_SESSION=$opt
          break
          ;;
      *)
          export MJM_SESSION="umpire"
          break
          ;;
  esac
done

. /usr/gapps/ares/bin/ares.bashrc
