#!/bin/bash
PS3='Please enter session choice: '
options=("ares" "umpire" "ompd")
select opt in "${options[@]}"
do
  case $opt in
      "ares")
          export MJM_SESSION=$opt
          break
          ;;
      "ompd")
          export MJM_SESSION=$opt
          break
          ;;
      "umpire")
          export MJM_SESSION=$opt
          break
          ;;
      *)
          export MJM_SESSION="umpire"
          break
          ;;
  esac
done
